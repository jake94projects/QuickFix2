<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.holdaway.jake.quickfix">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SoundActivity"
            android:label="Sound" />
        <activity
            android:name=".SecurityActivity"
            android:label="Security" />
        <activity
            android:name=".OsActivity"
            android:label="Operating System" />
        <activity
            android:name=".NetworkActivity"
            android:label="Network/Internet" />
        <activity
            android:name=".HardwareActivity"
            android:label="" />
        <activity
            android:name=".DeviceActivity"
            android:label="Device" />

        <activity android:name=".SearchActivity"
            android:label="Search"/>


        <activity android:name=".TroubleaudioActivity" />

        <activity android:name=".DevicemanagerActivity" />

        <activity android:name=".DefenderActivity" />

        <activity android:name=".NetworkadapterActivity" />

        <activity android:name=".WindowsrestoreActivity" />

        <activity android:name=".WindowsActivity"
            android:label="windows">

        </activity>
    </application>

</manifest>